//
//	currentTime : has no properties
//

global proc currentTimeProperties () 
{
	string $parent = `toolPropertyWindow -q -location`;
	setParent $parent;

	setUITemplate -pushTemplate OptionsTemplate;

	columnLayout -adj true currentTime;
		separator -style "none";
		text -label (uiRes("m_currentTimeProperties.kCurrentTimeNoSettingsDefined")) -align left text1;
	setParent ..;

	setUITemplate -popTemplate;
}
