global proc EEswitchSelectLayout(string $whichMethod)
{
	global string $gEEnodeMode;

	if ($gEEnodeMode == "scriptNode" || $whichMethod == "scriptNode") 
	{
		textField -edit -text "" EEexprNameT;
	}

	$gEEnodeMode = $whichMethod;

