        }
        setParent -menu ..;

	setParent -menu ..;

	menu -label (uiRes("m_expressionEditor.kHelp"))
		-helpMenu true EEhelpMenuItem;
		menuItem -label (uiRes("m_expressionEditor.kHelpExprEd")) 
			-enableCommandRepeat false
			-command "showHelp ExpressionEditor";

