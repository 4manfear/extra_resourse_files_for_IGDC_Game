<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEsimplexNoise'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='outAlpha' type='maya.float'>
			<label>Out Alpha</label>
		</attribute>
		<attribute name='uvCoord' type='maya.float2'>
			<label>UV Coord</label>
		</attribute>
		<attribute name='distortionU' type='maya.float'>
			<label>Distortion U</label>
		</attribute>
		<attribute name='distortionV' type='maya.float'>
			<label>Distortion V</label>
		</attribute>
	<view name='NEDefault' template='NEsimplexNoise'>
		<property name='outColor'/>
		<property name='outAlpha'/>
		<property name='uvCoord'/>
		<property name='distortionU'/>
		<property name='distortionV'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEsimplexNoise'>
		<property name='outColor'/>
	</view>
</templates>