
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEpointMatrixMult'>
		<attribute name='outMatrix' type='maya.matrix'>
			<label>Output Matrix</label>
		</attribute>
		<attribute name='inMatrix' type='maya.matrix'>
			<label>Input Matrix</label>
		</attribute>
        <attribute name='inPoint' type='maya.double3'>
			<label>Input Point</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEblendMatrix'>
		<property name='outMatrix'/>
		<property name='inMatrix'/>
        <property name='inPoint'/>
	</view>
</templates>
