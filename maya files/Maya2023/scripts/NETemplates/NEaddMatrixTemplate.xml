
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEaddMatrix'>
		<attribute name='matrixIn' type='maya.matrix'>
			<label>Input Matrix</label>
		</attribute>
		<attribute name='matrixSum' type='maya.matrix'>
			<label>Matrix Sum</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaddMatrix'>
		<property name='matrixSum'/>
		<property name='matrixIn'/>
	</view>
</templates>
