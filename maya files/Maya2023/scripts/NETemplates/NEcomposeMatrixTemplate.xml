
<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEcomposeMatrix'>
		<attribute name='inputQuat' type='maya.TdataCompound'>
			<label>Input Quat</label>
		</attribute>
		<attribute name='inputRotate' type='maya.double3'>
			<label>Input Rotate</label>
		</attribute>
		<attribute name='inputScale' type='maya.double3'>
			<label>Input Scale</label>
		</attribute>
		<attribute name='inputShear' type='maya.double3'>
			<label>Input Shear</label>
		</attribute>
		<attribute name='inputTranslate' type='maya.double3'>
			<label>Input Translate</label>
		</attribute>
		<attribute name='outputMatrix' type='maya.matrix'>
			<label>Output Matrix</label>
		</attribute>
		<attribute name='outputRotateOrder' type='maya.enum'>
			<label>Output Rotate Order</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEcomposeMatrix'>
		<property name='inputQuat'/>
		<property name='inputTranslate'/>
		<property name='inputRotate'/>
		<property name='inputScale'/>
		<property name='inputShear'/>
		<property name='outputMatrix'/>
		<property name='outputRotateOrder'/>
	</view>
</templates>
