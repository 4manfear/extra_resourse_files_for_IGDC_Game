//
//	Description Name;
//	Creates the attribute editor controls for the alignSurface Node
//
//	Input Value:
//	nodeName
//
//	Output Value:
//	None
//

global proc AEalignSurfaceTemplate( string $nodeName )
{

	AEalignCommon $nodeName surface;

	editorTemplate -suppress "inputSurface1";
	editorTemplate -suppress "inputSurface2";

}
