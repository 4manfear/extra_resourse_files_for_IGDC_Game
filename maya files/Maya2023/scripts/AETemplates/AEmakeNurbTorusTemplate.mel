//	Procedure Name:
//	AEmakeNurbTorusTemplate
//
//	Description Name;
//	Creates the attribute editor controls for the makeNurbTorus node
//
//	Input Value:
//	nodeName
//
//	Output Value:
//	None
//

global proc AEmakeNurbTorusTemplate( string $nodeName )
{
	
	editorTemplate -beginScrollLayout;

	editorTemplate -beginLayout (uiRes("m_AEmakeNurbTorusTemplate.kTorusHistory")) -collapse false;
		// include/call base class/node attributes
		AErevolvedPrimitiveCommon $nodeName;

		editorTemplate -addControl "minorSweep";
	editorTemplate -endLayout;
	
	// include/call base class/node attributes
	AErevolvedPrimitiveInclude $nodeName;

	editorTemplate -addExtraControls;
	editorTemplate -endScrollLayout;

}
