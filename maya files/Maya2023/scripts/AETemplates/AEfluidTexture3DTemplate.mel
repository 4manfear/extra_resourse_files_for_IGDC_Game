//
//	Procedure Name:
//	AEfluidTextureTemplate
//
//	Description
//		Creates the attribute editor controls for the fluidTexture node
//
//	Input Value:
//		nodeName
//
//	Output Value:
//		None
//
global proc AEfluidTexture3DTemplate( string $nodeName )
{
	AEfluidAttrTemplate( $nodeName, "fluidTexture3D" );
	editorTemplate -suppress "pointCamera";
	editorTemplate -suppress mentalRayControls;
}

