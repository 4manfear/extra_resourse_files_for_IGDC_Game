//
//  Procedure Name:
//	AEsurfaceVarGroupTemplate
//
//  Description Name;
//	Creates the attribute editor controls for the surfaceVarGroup Node
//
//  Input Value:
//	nodeName 
//
//  Output Value:
//	None
// 


global proc AEsurfaceVarGroupTemplate ( string $nodeName )
{
	editorTemplate -beginScrollLayout;


		// suppressed attributes
		editorTemplate -suppress "create";

		// include/call base class/node attributes
		AEgeometryVarGroupInclude $nodeName;

	editorTemplate -addExtraControls;
	editorTemplate -endScrollLayout;
}
