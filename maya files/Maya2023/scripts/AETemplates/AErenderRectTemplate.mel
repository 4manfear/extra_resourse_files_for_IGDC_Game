//
//  Procedure Name:
//	AErenderRectTemplate
//
//  Description:
//	Creates the attribute editor controls for 
// 	the renderRect Node
//
//  Input Value:
//	nodeName
//
//  Output Value:
//	None
//


//
//  Procedure Name
//	AErenderRectTemplate
//

global proc AErenderRectTemplate( string $nodeName )
{
	editorTemplate -beginScrollLayout;
	
	// include/call base class/node attributes
	AEgeometryShapeTemplate $nodeName;
	
	editorTemplate -addExtraControls;
	editorTemplate -endScrollLayout;

}
