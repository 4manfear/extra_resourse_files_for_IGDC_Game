//
//  Procedure Name:
//	AEcreateUVSetTemplate
//
//  Description Name;
//	Creates the attribute editor controls for the deleteUVSet Node
//
//  Input Value:
//	nodeName 
//
//  Output Value:
//	None
// 


global proc AEcreateUVSetTemplate ( string $nodeName )
{
    editorTemplate -beginScrollLayout;

    // include/call base class/node attributes      
    AEpolyBaseTemplate $nodeName;

	editorTemplate -suppress "inputGeometry";
	editorTemplate -suppress "outputGeometry";
	editorTemplate -suppress "uvSetName";
	
    editorTemplate -addExtraControls;
    editorTemplate -endScrollLayout;

}
