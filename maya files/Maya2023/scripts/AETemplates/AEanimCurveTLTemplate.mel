//
//	Procedure Name:
//	AEanimCurveTLTemplate
//
//	Description Name;
//	Creates the attribute editor controls for the animCurveTL Node
//
//	Input Value:
//	nodeName
//
//	Output Value:
//	None
//

global proc AEanimCurveTLTemplate( string $nodeName )
{
	editorTemplate -beginScrollLayout;

		// include/call base class/node attributes
		AEanimCurveTemplate($nodeName);


	// suppressed attributes
	editorTemplate -suppress "input";

	editorTemplate -addExtraControls;
	editorTemplate -endScrollLayout;
}
