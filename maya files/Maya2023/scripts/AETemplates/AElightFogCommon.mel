//
//  Procedure Name:
//	AElightFogCommon
//
//  Description Name;
//	Creates the attribute editor controls for the lightFog Node
//
//  Input Value:
//	nodeName 
//
//  Output Value:
//	None
// 


global proc AElightFogCommon ( string $nodeName )
{
		
	editorTemplate -addControl "color";
	editorTemplate -addControl "density";
	editorTemplate -addControl "colorBasedTransparency";
	editorTemplate -interruptOptimize;
	editorTemplate -addControl "fastDropOff";
}
