// Attribute Editor Template for render setup render layer node.
//
global proc AErenderSetupLayerTemplate(string $nodeName) {
	editorTemplate -beginScrollLayout;
		editorTemplate -suppress "parentList";
		editorTemplate -suppress "previous";
		editorTemplate -suppress "containerLowest";
		editorTemplate -suppress "containerHighest";
		editorTemplate -suppress "legacyRenderLayer";
		editorTemplate -addExtraControls;
	editorTemplate -endScrollLayout;
}
