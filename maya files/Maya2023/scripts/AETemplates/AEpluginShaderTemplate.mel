//
//  Procedure Name:
//	AEpluginShaderTemplate
//
//  Description Name;
//	Creates the attribute editor controls for pluginShader Nodes
//
//  Input Value:
//	nodeName 
//
//  Output Value:
//	None
// 


global proc AEpluginShaderTemplate ( string $nodeName )
{
	AEswatchDisplay $nodeName;
	editorTemplate -beginScrollLayout;

		// include/call base class/node attributes
		AEdependNodeTemplate $nodeName;

	editorTemplate -addExtraControls;
	editorTemplate -endScrollLayout;
}
