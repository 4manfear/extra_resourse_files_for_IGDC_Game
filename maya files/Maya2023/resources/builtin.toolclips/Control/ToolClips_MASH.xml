<?xml version="1.0" encoding="utf-8"?>
<toolclips ui="ui/ToolClipUI.qml" simple="ui/SimpleTooltip.qml" locale="en_US">
  <toolclip key="MASHVisibilityBtn" title="Visibility" help="MASH_Visibility">
    <text>Alters the visibility of instanced objects.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_visibility.webm">Visibility</video>
        <link location="MASH_Visibility">Visibility</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHTrailsBtn" title="Trails" help="MASH_Trails">
    <text>Creates a trail of quads behind or between points.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_trails.webm">Trails</video>
        <link location="MASH_Trails">Trails</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHTimeBtn" title="Time" help="MASH_Time">
    <text>Used to offset the component animation (i.e. deformations) of the meshes created when Geometry Type is set to Mesh. </text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_time.webm">Time</video>
        <link location="MASH_Time">Time</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHSpringBtn" title="Spring" help="MASH_Spring">
    <text>Adds spring to an existing animation.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_spring">Spring</video>
        <link location="MASH_Spring">Spring</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHReplicateBtn" title="Replicate" help="MASH_Replicator">
    <text>Duplicates the MASH network and allows you to perform transformations on the replicants.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_replicate.webm">Pinch UVs</video>
        <link location="MASH_Replicate">Replicate</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHOrientBtn" title="Orient" help="MASH_Orient">
    <text>Allows you to aim points in a particular direction.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_orient.webm">Orient</video>
        <link location="MASH_Orient">Orient</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHSignalBtn" title="Signal" help="MASH_Signal">
    <text>Animates the points in your network using either 4D Perlin Noise or a Trigonometric function.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_Signal.webm">Signal</video>
        <link location="MASH_Signal">Signal</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHStrengthBtn" title="Strength" help="MASH_Strength">
    <text>Controls the strength of attached nodes.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_strength.webm">Strength</video>
        <link location="MASH_Strength">Strength</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHSymmetryBtn" title="Symmetry" help="MASH_Symmetry">
    <text>Joins the selected edges two UV shells by moving, rotating, or scaling one to match the other.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_symmetry.webm">Symmetry</video>
        <link location="MASH_Symmetry">Symmetry</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHInfluenceBtn" title="Influence" help="MASH_Influence">
    <text>Influences the scale and rotation of object instances according to the scale and rotation of connected influence locators.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_influence.webm">Influence</video>
        <link location="MASH_Influence">Influence</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHExplodeBtn" title="Explode" help="MASH_Explode">
    <text>Extracts a mesh's faces and transforms them according to existing MASH points.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_explode.webm">Explode</video>
        <link location="MASH_Explode">Explode</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHFlightBtn" title="Flight" help="MASH_Flight">
    <text>Multi-threaded simulation node that approximates flocking and swarming behavior by attaching objects to a number of points.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_flight.webm">Flight</video>
        <link location="MASH_Flight">Flight</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHDelayBtn" title="Delay" help="MASH_Delay">
    <text>Allows you to inherit the animation of an object, then offset it in time.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_delay.webm">Delay</video>
        <link location="MASH_Delay">Delay</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHCurveBtn" title="Curve" help="MASH_Curve">
    <text>Distributes and animates MASH points along input curves.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_curve.webm">Curve</video>
        <link location="MASH_Curve">Curve</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHColorBtn" title="Color" help="MASH_Color">
    <text>Customizes the display of a MASH network's CPV data in the workspace for meshes created when the Geometry Type is set to Mesh.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_color.webm">Color</video>
        <link location="MASH_Color">Color</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHAudioBtn" title="Audio" help="MASH_Audio">
    <text>Power animation of points with an audio file.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_audio.webm">Audio</video>
        <link location="MASH_Audio">Audio</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHPlacerBtn" title="Placer" help="MASH_Placer"> 
    <text>Place MASH points by painting.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_Placer.webm">Placer</video>
        <link location="MASH_Placer">Placer</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="MASHDynamicsBtn" title="Dynamics" help="MASH_Dynamics"> 
    <text>Make MASH points dynamic using Maya Bullet.</text>
    <expanded>
      <default-resource index="1"/>
      <resourcelist>
        <video location="MASH_Dynamics.webm">Dynamics</video>
        <link location="MASH_Dynamics">Dynamics</link>
        <link location="MASH_BulletSolver">BulletSolver</link>
	<link location="MASH_Constraint">Constraint</link>
      </resourcelist>
    </expanded>
  </toolclip>
 
  
  
</toolclips>
